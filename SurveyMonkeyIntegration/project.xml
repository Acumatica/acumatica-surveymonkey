    <Sql TableName="updateCRCase" CustomScript="#CDATA">
        <CDATA name="CustomScript"><![CDATA[
	
IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrResponseID' and Object_ID = Object_ID(N'CRCase'))
BEGIN
	ALTER TABLE CRCase ADD UsrResponseID BIGINT NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrResponseDate' and Object_ID = Object_ID(N'CRCase'))
BEGIN
	ALTER TABLE CRCase ADD UsrResponseDate DATETIME NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrInternalComment' and Object_ID = Object_ID(N'CRCase'))
BEGIN
	ALTER TABLE CRCase ADD UsrInternalComment NVARCHAR(255) NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrInternalCommentByName' and Object_ID = Object_ID(N'CRCase'))
BEGIN
	ALTER TABLE CRCase ADD UsrInternalCommentByName NVARCHAR(255) NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrSurveyReportingEligible' and Object_ID = Object_ID(N'CRCase'))
BEGIN
	ALTER TABLE CRCase ADD UsrSurveyReportingEligible BIT NULL
END
GO

]]></CDATA>
    </Sql>
