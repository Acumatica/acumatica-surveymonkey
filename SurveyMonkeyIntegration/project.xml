IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrAccessToken' and Object_ID = Object_ID(N'CRSetup'))
BEGIN
	ALTER TABLE CRSetup ADD UsrAccessToken NVARCHAR(300) NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrSurveyClientID' and Object_ID = Object_ID(N'CRSetup'))
BEGIN
	ALTER TABLE CRSetup ADD UsrSurveyClientID NVARCHAR(100) NULL
END
GO

IF NOT EXISTS(SELECT * FROM Sys.Columns WHERE Name = N'UsrSurveyClientSecret' and Object_ID = Object_ID(N'CRSetup'))
BEGIN
	ALTER TABLE CRSetup ADD UsrSurveyClientSecret NVARCHAR(100) NULL
END
GO]]></CDATA>
    </Sql>
</Customization>